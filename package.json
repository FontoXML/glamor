{
  "name": "glamor",
  "version": "2.20.40",
  "description": "inline css for component systems",
  "main": "lib/index.js",
  "typings": "index.d.ts",
  "files": [
    "lib",
    "umd",
    "index.d.ts"
  ],
  "scripts": {
    "clean": "rimraf lib umd",
    "build": "npm-run-all -s clean -p build:**",
    "build:lib": "babel src -d lib --presets env,stage-0",
    "build:umd": "mkdirp umd && webpack --config webpack/umd.js",
    "build:umd-prod": "cross-env NODE_ENV=production npm run build:umd",
    "prepublish": "npm run build"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.1",
    "babel-preset-env": "^1.6.0",
    "babel-preset-stage-0": "^6.24.1",
    "cross-env": "^5.0.1",
    "mkdirp": "^0.5.1",
    "mocha": "^3.4.2",
    "npm-run-all": "^4.0.2",
    "rimraf": "^2.6.1",
    "webpack": "^3.3.0"
  },
  "author": "Sunil Pai <threepointone@gmail.com>",
  "homepage": "https://github.com/threepointone/glamor",
  "license": "MIT"
}
